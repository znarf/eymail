{
  "name": "@eymail/editor",
  "version": "0.0.1",
  "description": "EyeMail Editor",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"client/**/*.js\" \"editor/**/*.js\"  \"server/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "prettier": "prettier \"*.@(js|json|md)\" \"client/**/*.@(js|json|md)\" \"editor/**/*.@(js|json|md)\"  \"server/**/*.@(js|json|md)\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --list-different"
  },
  "engines": {
    "node": "11.8.0",
    "npm": "6.5.0"
  },
  "dependencies": {
    "@eymail/builder": "^0.0.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "body-parser": "^1.18.3",
    "codemirror": "^5.45.0",
    "express": "^4.16.4",
    "hbs": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-resolver": "^3.1.2",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-opencollective": "^1.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4"
  }
}
